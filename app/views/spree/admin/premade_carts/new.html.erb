<% admin_breadcrumb link_to(plural_resource_name(Spree::PremadeCart), spree.admin_premade_carts_path) %>

<%= render partial: 'spree/shared/error_messages', locals: { target: @premade_cart } %>

<%= form_for [:admin, @premade_cart], method: :post, url: admin_premade_carts_path do |f| %>
  <fieldset data-hook="new_premade_cart">
    <legend align="center"><%= t('spree.new_premade_cart') %></legend>

    <%= render partial: 'form', locals: { f: f } %>

    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
